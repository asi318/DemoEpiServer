@using DemoEpiServer
@model PageViewModel<CareerPage> 

<h1 @Html.EditAttributes(x => x.CurrentPage.MetaTitle)>@Model.CurrentPage.MetaTitle</h1>

<div class="row">
    <div class="col-md-12 clearfix" @Html.EditAttributes(x => x.CurrentPage.CareerContent)>
        <p class="introduction" @Html.EditAttributes(x => x.CurrentPage.MetaDescription)>@Model.CurrentPage.MetaDescription</p>
        @Html.DisplayFor(m => m.CurrentPage.CareerContent)
</div>
</div>

